# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8

[*.java]
indent_style = tab
max_line_length = 180

[*.json]
indent_style = space
indent_size = 2

[*.xml]
indent_style = space
indent_size = 4

